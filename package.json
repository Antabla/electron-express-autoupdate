{
  "name": "electron-with-express",
  "description": "Demonstrates spawning Express from Electron and using it to serve content",
  "version": "0.11.11",
  "main": "main.js",
  "author": "Antabla",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "publish": "build -p always",
    "start": ".\\node.exe start-electron.js"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "keymaster": "^1.6.2",
    "lodash": "^4.17.11",
    "request": "^2.88.0",
    "strip": "^3.0.0",
    "electron-log": "^1.3.0",
    "electron-updater": "^4.0.6"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^20.28.4"
  },
  "build": {
    "appId": "com.github.iffy.electronupdaterexample",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico",
      "publish": {
        "provider": "s3",
        "bucket": "appelectron1.0",
        "region":"sa-east-1"
      }
    }
  }
}